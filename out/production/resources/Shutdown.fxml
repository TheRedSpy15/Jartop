         <font>
            <Font name="Tahoma" size="22.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="yesBtn" buttonType="RAISED" focusTraversable="false" layoutX="163.0" layoutY="101.0" onAction="#shutdown" prefHeight="51.0" prefWidth="113.0" style="-fx-background-color: white;" text="Yes">
         <font>
            <Font name="Tahoma" size="22.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
