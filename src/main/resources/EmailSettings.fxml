<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXToggleButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="background" prefHeight="441.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="EmailSettings">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Settings">
            <font>
                <Font name="Tahoma Bold" size="35.0" />
            </font>
        </Label>
        <Separator layoutY="58.0" prefHeight="6.0" prefWidth="600.0" />
        <JFXTextField fx:id="addressField" focusTraversable="false" labelFloat="true" layoutX="14.0" layoutY="85.0" prefHeight="31.0" prefWidth="573.0" promptText="Address">
            <font>
                <Font name="Tahoma" size="18.0" />
            </font>
        </JFXTextField>
        <JFXButton fx:id="applyBtn" focusTraversable="false" layoutX="480.0" layoutY="393.0" onAction="#apply" prefHeight="38.0" prefWidth="114.0" text="Apply">
            <font>
                <Font name="Tahoma" size="23.0" />
            </font>
        </JFXButton>
        <JFXButton fx:id="backBtn" focusTraversable="false" layoutX="4.0" layoutY="393.0" onAction="#back" prefHeight="44.0" prefWidth="114.0" text="Back">
            <font>
                <Font name="Tahoma" size="23.0" />
            </font>
        </JFXButton>
        <JFXToggleButton fx:id="sslToggle" focusTraversable="false" layoutX="245.0" layoutY="302.0" text="SSL">
            <font>
                <Font name="Tahoma" size="19.0" />
            </font>
        </JFXToggleButton>
      <JFXTextField fx:id="passwordField" focusTraversable="false" labelFloat="true" layoutX="14.0" layoutY="138.0" prefHeight="31.0" prefWidth="573.0" promptText="Password">
         <font>
            <Font name="Tahoma" size="18.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="providerField" focusTraversable="false" labelFloat="true" layoutX="14.0" layoutY="190.0" prefHeight="31.0" prefWidth="573.0" promptText="Provider">
         <font>
            <Font name="Tahoma" size="18.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="portField" focusTraversable="false" labelFloat="true" layoutX="14.0" layoutY="247.0" prefHeight="31.0" prefWidth="573.0" promptText="Port">
         <font>
            <Font name="Tahoma" size="18.0" />
         </font>
      </JFXTextField>
    </children>
</AnchorPane>
