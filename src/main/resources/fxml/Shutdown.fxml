<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<HBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ShutdownController">
   <children>
    <Button focusTraversable="false" mnemonicParsing="false" onAction="#cancel" text="Cancel" HBox.hgrow="ALWAYS" />
      <Button focusTraversable="false" mnemonicParsing="false" onAction="#shutdown" text="Shutdown" />
      <Button focusTraversable="false" mnemonicParsing="false" onAction="#signOut" text="Sign Out" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</HBox>
