      </JFXToggleButton>
      <JFXToggleButton fx:id="httpsToggle" focusTraversable="false" layoutX="12.0" layoutY="253.0" text="https Only">
         <font>
            <Font name="Tahoma" size="19.0" />
         </font>
      </JFXToggleButton>
      <JFXButton fx:id="resetBtn" layoutX="165.0" layoutY="20.0" onAction="#reset" prefHeight="33.0" prefWidth="69.0" text="reset">
