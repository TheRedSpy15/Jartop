<AnchorPane fx:id="background" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BrowserSettingsController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Settings" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="14.0">
