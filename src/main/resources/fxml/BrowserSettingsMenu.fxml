            <Font name="Tahoma Bold" size="35.0" />
         </font>
      </Label>
      <Separator layoutY="58.0" />
      <Label layoutX="12.0" layoutY="73.0" text="Max History:" AnchorPane.leftAnchor="12.0" AnchorPane.topAnchor="73.0">
         <font>
            <Font name="Tahoma" size="19.0" />
         </font>
      </Label>
      <JFXSlider fx:id="historySlider" layoutX="129.0" layoutY="77.0" snapToTicks="true" AnchorPane.leftAnchor="135.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="77.0" />
      <JFXToggleButton fx:id="javaScriptToggle" focusTraversable="false" layoutX="14.0" layoutY="189.0" text="Javascript">
         <font>
            <Font name="Tahoma" size="19.0" />
         </font>
      </JFXToggleButton>
      <JFXToggleButton fx:id="popUpToggle" disable="true" focusTraversable="false" layoutX="12.0" layoutY="317.0" text="Block Pop-ups">
