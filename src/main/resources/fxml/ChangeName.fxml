<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:id="background" prefHeight="210.0" prefWidth="275.0" style="-fx-background-color: #cedede;" xmlns="http://javafx.com/javafx/10.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ChangeName">
   <children>
      <JFXTextField fx:id="nameField" focusTraversable="false" labelFloat="true" layoutX="24.0" layoutY="46.0" onAction="#change" prefHeight="33.0" prefWidth="227.0" promptText="Name">
         <font>
            <Font name="Tahoma" size="19.0" />
         </font>
      </JFXTextField>
      <JFXButton fx:id="changeBtn" buttonType="RAISED" focusTraversable="false" layoutX="95.0" layoutY="153.0" onAction="#change" prefHeight="36.0" prefWidth="85.0" style="-fx-background-color: white;" text="Change">
         <font>
            <Font name="Tahoma" size="18.0" />
         </font>
      </JFXButton>
      <JFXToggleButton fx:id="fileNameToggle" focusTraversable="false" layoutX="46.0" layoutY="95.0" text="Change file name too" />
   </children>
</AnchorPane>
